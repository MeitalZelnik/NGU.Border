             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:NGU.App.Client.Views"
             mc:Ignorable="d" 
             d:DesignHeight="300" d:DesignWidth="300">
    <Grid>
        <StackPanel >
            <TextBlock>View 12 the Queen</TextBlock>
            <Label FontSize="32" HorizontalAlignment="Center">Biometrics Page</Label>
            <Button Command="{Binding CaptureCommand}" Content="Capture" FontWeight="Bold" Width="150" Height="50" Margin="50"/>
        </StackPanel>    
    </Grid>
</UserControl>
